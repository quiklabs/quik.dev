{
    "name": "@quik/server",
    "version": "0.0.1",
    "main": "dist/index.js",
    "scripts": {
        "prestart": "figlet \"quik.dev\"",
        "start": "concurrently -n build,run \"pnpm run build:watch\" \"pnpm run start:watch\"",
        "start:watch": "CI=true node --watch --inspect dist/index.js | pino-pretty",
        "start:prod": "node dist/index.js",
        "build": "rollup --config rollup.config.js --bundleConfigAsCjs",
        "build:watch": "CI=true rollup --watch --config rollup.config.js --bundleConfigAsCjs",
        "build:prod": "rollup --config rollup.config.js --bundleConfigAsCjs",
        "lint": "eslint src/**/* --ext js,ts,tsx,jsx",
        "migrate:reset": "prisma migrate reset",
        "migrate:create": "prisma migrate dev --create-only",
        "migrate": "prisma migrate deploy"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.0.1",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/lodash": "^4.14.200",
        "@types/node": "^20.7.0",
        "@types/pg": "^8.10.7",
        "@types/pg-format": "^1.0.4",
        "@vercel/ncc": "^0.38.1",
        "chokidar-cli": "^3.0.0",
        "concurrently": "^8.2.2",
        "figlet-cli": "^0.1.1",
        "nodemon": "^3.0.1",
        "pino-pretty": "^10.2.3",
        "rollup": "^4.1.5",
        "rollup-plugin-filesize": "^10.0.0",
        "rollup-plugin-visualizer": "^5.9.2",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "ascii-text-generator": "^1.0.5",
        "dotenv": "^16.3.1",
        "fastify": "^4.24.2",
        "lodash": "^4.17.21",
        "pg": "^8.11.3",
        "pg-format": "^1.0.4",
        "pino": "^8.16.0",
        "prisma": "^5.4.2",
        "typia": "^5.2.3"
    }
}
